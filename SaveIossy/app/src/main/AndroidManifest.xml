<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.saveiossy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SaveIossy">
        <activity android:name=".InSharatclass"></activity>
        <activity android:name=".SharatClass" />
        <activity android:name=".GlowingScreen" />
        <activity android:name=".InAppclass" />
        <activity android:name=".Loading" />
        <activity android:name=".EndScreen" />
        <activity android:name=".ComplibScreen" />
        <activity android:name=".Complib" />
        <activity android:name=".InLibrary" />
        <activity android:name=".Library" />
        <activity android:name=".Appclass" />
        <activity android:name=".Appcor" />
        <activity android:name=".Credits" />
        <activity android:name=".NameInput" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>